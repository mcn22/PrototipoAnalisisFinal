<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Energym</title>

    <!--=======================================
      All Css Style link
    ===========================================-->

    <!-- Custom styles for this template -->
    <link href="css/style.css" rel="stylesheet">
    <link href="css/bootstrap-social.css" rel="stylesheet" >

    <!-- FAVICONS -->
    <link rel="icon" href="img/favicon-16x16.png" type="image/png" sizes="16x16">
    <link rel="shortcut icon" href="img/favicon-16x16.png" type="image/x-icon">
    <link rel="shortcut icon" href="img/favicon-16x16.png">

    <link rel="apple-touch-icon-precomposed" type="image/x-icon" href="img/apple-icon-72x72.png"
        sizes="72x72" />
    <link rel="apple-touch-icon-precomposed" type="image/x-icon" href="img/apple-icon-114x114.png"
        sizes="114x114" />
    <link rel="apple-touch-icon-precomposed" type="image/x-icon" href="img/apple-icon-144x144.png"
        sizes="144x144" />
    <link rel="apple-touch-icon-precomposed" type="image/x-icon" href="img/favicon-16x16.png" />

    <script src="js/jquery-3.6.0.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="js/navbarIn.js"></script>
</head>

<body>

    <!-- Pre Loader Area start -->
    <div id="preloader">
        <div class="loader"></div>
    </div>
    <!-- Pre Loader Area End -->

    <!-- Menu Area Start -->
    <section class="menu-section-area padding-zero">
        <!-- Navigation -->
        <nav class="navbar navbar-expand-lg navbar-dark fixed-top d-none d-sm-none d-md-block d-lg-block d-xl-block"
            id="mainNav">

        </nav>
        <!-- Navigation -->

        <!-- Mobile Menu Start -->
        <nav class="mobile_menu hidden d-none ">
            <div id="navegacionMobile">

            </div>
        </nav>
        <!-- Mobile Menu End -->
    </section>
    <!-- Menu Area End -->

    <!-- Calculate BMI Area Start -->
    <section class="calculate-bmi-area">
        <div class="container">
            <!--Row Start-->
            <div class="row">

                <!-- Column Start-->
                <div class="col-sm-12">
                    <h2 class="para-color mt-5">Registro de Cliente</h2>
                </div>
                <!-- Column End-->

            </div>
            <!--Row End-->

            <!--Row Start-->
            <div class="row ">
                <!-- Column Start-->
                <div class="col-sm-8">
                    <h4 class="para-color mt-4">Información requerida</h4>
                    <div class="bmi-box">
                        <form action="/register" method="POST" onsubmit="return validateData()">
                            <div class="row">
                                <div class="col-sm-6 mt-3">
                                    <div class="form-group">
                                        <label for="cus_name">Nombre<span>*</span></label>
                                        <input type="text" class="form-control" placeholder="" id="height" name="fName">
                                    </div>
                                </div>
                                <div class="col-sm-6 mt-3">
                                    <div class="form-group">
                                        <label for="cus_name">Apellido<span>*</span></label>
                                        <input type="text" class="form-control" placeholder="" id="height" name="lName">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label for="cus_name">Telefono<span>*</span></label>
                                        <input type="text" class="form-control" placeholder="" id="height" name="phone">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label for="cus_name">Correo electrónico<span>*</span></label>
                                        <input type="text" class="form-control" placeholder="" id="height" name="username">
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label for="cus_name">Fecha de nacimiento<span>*</span></label>
                                        <input type="date" class="form-control" placeholder="" id="height" name="bDate">
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label for="cus_name">Provincia<span>*</span></label>
                                        <select name="province" class="form-control custom-select">
                                            <option>Selecciona la provincia</option>
                                            <option value="1">San Jose</option>
                                            <option value="2">Alajuela</option>
                                            <option value="3">Cartago</option>
                                            <option value="4">Heredia</option>
                                            <option value="5">Guanacaste</option>
                                            <option value="6">Puntarenas</option>
                                            <option value="7">Limon</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group">
                                        <label for="cus_name">Codigo postal<span>*</span></label>
                                        <input type="text" class="form-control" placeholder="" id="height" name="postalCode">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label for="cus_name">Direccion<span>*</span></label>
                                        <input type="text" class="form-control" placeholder="" id="height" name="address">
                                    </div>
                                    <hr class="my-4">
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label for="cus_name">Contraseña<span>*</span></label>
                                        <input id="fPassword" type="text" class="form-control" placeholder="" id="height" name="password">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label for="cus_name">Confirme contraseña<span>*</span></label>
                                        <input id="sPassword" type="text" class="form-control" placeholder="" id="height" >
                                    </div>
                                </div>

                                <div class="col-sm-12">
                                    <div class="contact-sub-btn">
                                        <button type="submit" class="btn btn-effect section-button">Registrar</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-sm-4">
                    <h4 class="para-color mt-4 mb-4">Códigos Promocionales</h4>
                    <div class="bmi-box">
                          <ul class="list-group mb-3">
                            <li class="list-group-item d-flex justify-content-between bg-light mt-3">
                              <div class="text-success">
                                <h6 class="my-0">Promo code</h6>
                                <small>EXAMPLECODE</small>
                              </div>
                              <span class="text-success">−₡5000</span>
                            </li>
                          </ul>

                          <form class="card p-2">
                            <div class="input-group">
                              <input type="text" class="form-control" placeholder="Promo code">
                              <button type="submit" class="btn btn-secondary">Redimir</button>
                            </div>
                          </form>
                    </div>
                </div>
                <!-- Column End-->
            </div>
            <!--Row End-->
        </div>
    </section>
    <!-- Calculate BMI Area End -->

    <!-- Footer Start -->
    <footer class="footer">
        <!-- Footer Bottom Area Start-->
        <div class="footer-bottom-area">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="footer-bottom-part">
                            <div class="copyright"><a href="https://anditthemes.com/">Energym</a> &copy; 2021. All
                                Rights Reserved</div>
                            <div class="footer-social">
                                <ul>
                                    <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="#"><i class="fab fa-linkedin-in"></i></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--  Footer Bottom Area End-->

    </footer>
    <!-- Footer End -->

    <!--Top Arrow area start-->
    <div class="top-arrow">
        <a href="#" id="scroll" style="display: none;"><i class="fas fa-angle-up"></i></a>
    </div>
    <!--Top Arrow area End-->

    <!--=======================================
          All Jquery Script link
    ===========================================-->

    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/jquery/popper.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="js/validation.js" type="text/javascript">

    </script>

    <!-- ==== Plugin JavaScript ==== -->

    <!-- jQuery owl carousel -->
    <script src="js/owl.carousel.min.js"></script>

    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!--  Counter Up JavaScript-->
    <script src="js/waypoints.min.js"></script>
    <script src="js/jquery.counterup.min.js"></script>

    <!--Parallax Script-->
    <script src="js/TweenMax.min.js"></script>

    <!--VenoBox Script-->
    <script src="js/venobox.min.js"></script>

    <!--WOW JS Script-->
    <script src="js/wow.min.js"></script>

    <!--AOS JS Script-->
    <script src="js/aos.js"></script>

    <!--Paroller JS Script-->
    <script src="js/jquery.paroller.min.js"></script>

    <!--Mean Menu/Mobile Menu Script-->
    <script src="js/jquery.meanmenu.min.js"></script>

    <!-- Custom scripts for this template -->
    <script src="js/custom.js"></script>

</body>

</html>
